{
  "name": "@igloo/igloo-amaging",
  "version": "0.1.2",
  "main": "lib/amaging/server.js",
  "scripts": {
    "test": "npm run lint && npm run test:local",
    "test:mocha": "mocha --require coffee-script/register src/test/*_test.coffee",
    "test:local": "TEST_ENV=local npm run test:mocha",
    "test:s3": "TEST_ENV=s3 npm run test:mocha",
    "lint": "coffeelint src",
    "start": "nodemon src/samples/local/sample.coffee",
    "start:s3": "nodemon src/samples/s3/sample.coffee",
    "prepublish": "npm run build",
    "build": "coffee -c -o ./lib src"
  },
  "devDependencies": {
    "aws-sdk": "~2.6.6",
    "chai": "~3.5.0",
    "coffee-script": "^1.11.0",
    "coffeelint": "^1.15.7",
    "copy": "^0.3.0",
    "longjohn": "^0.2.9",
    "mocha": "^3.1.0",
    "nodemon": "^1.10.2",
    "request": "~2.75.0",
    "supertest": "~2.0.0"
  },
  "dependencies": {
    "async": "~2.0.1",
    "boom": "^4.1.0",
    "cors": "^2.8.1",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "formidable": "^1.0.17",
    "gm": "~1.23.0",
    "@igloo/igloo-amaging-policy": "0.1.1",
    "knox": "^0.9.2",
    "lodash": "~4.16.2",
    "mime": "~1.3.4",
    "mkdirp": "~0.5.1",
    "nconf": "^0.8.4",
    "rimraf": "~2.5.4",
    "tmp": "0.0.29"
  }
}
